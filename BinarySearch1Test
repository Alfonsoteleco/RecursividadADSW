import static org.junit.Assert.*;

import org.junit.Test;

public class BinarySearch1Test {
          @Test
        public void testbinarySearch () {
            BinarySearch1 prueba = new BinarySearch1();
            int lista[] = {1,2,3,4,5,6,9,10,48};
                    assertEquals(-1,prueba.binarySearch(lista,0,8,-4));

        }
        @Test
        public void testbinarySearch2 () {
            BinarySearch1 prueba = new BinarySearch1();
            int lista[] = {1,2,3,4,5,6,9,10,48};
            assertEquals(5,prueba.binarySearch(lista,0,8,5));

        }
        @Test
        public void testbinarySearch3 () {
            BinarySearch1 prueba = new BinarySearch1();
            int lista[] = {1,2,3,4,5,6,9,10,48};
            assertEquals(-1,prueba.binarySearch(lista,0,8,50));

        
    }
}
